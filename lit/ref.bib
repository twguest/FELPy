	file = {Ashuri and Bar-Ilan - 2015 - How flat organizations filter organizational gate.pdf:/home/twguest/Zotero/storage/7EE7VVUG/Ashuri and Bar-Ilan - 2015 - How flat organizations filter organizational gate.pdf:application/pdf}
}

