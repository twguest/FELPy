	number = {28},
	urldate = {2019-02-07},
	journal = {Journal of the American Chemical Society},
	author = {Chen, Chih-Jung and Pang, Wei Kong and Mori, Tatsuhiro and Peterson, Vanessa K. and Sharma, Neeraj and Lee, Po-Han and Wu, She-huang and Wang, Chun-Chieh and Song, Yen-Fang and Liu, Ru-Shi},
