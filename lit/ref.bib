@inproceedings{naulleau_accelerating_2011,
	address = {Monterey, California},
	title = {Accelerating {EUV} learning with synchrotron light: mask roughness challenges ahead},
	shorttitle = {Accelerating {EUV} learning with synchrotron light},
	url = {http://proceedings.spiedigitallibrary.org/proceeding.aspx?doi=10.1117/12.900488},
	doi = {10.1117/12.900488},
	abstract = {Despite significant progress in the commercialization of extreme ultraviolet (EUV) lithography, many important challenges remain, including in the area of masks. The issue of EUV phase roughness that can arise from either multilayer or capping layer roughness has recently garnered increasing concern. The problem with mask phase roughness is that it couples line-edge roughness (LER) through the formation of image plane speckle. The coupling from phase roughness to LER depends on many factors including roughness magnitude, roughness correlation length, illumination partial coherence, aberrations, defocus, and numerical aperture. Analysis shows that only on the order of 50 pm multilayer roughness may be tolerable at the 22-nm half-pitch node. Results also show that Atomic Force Microscopy (AFM) may not be a suitable method for measuring mask phase roughness due to its sensitivity to the surface only. Capping layer roughness is another significant concern especially given that it has been shown to increase with cleaning cycles. In this case, however, AFM does provide a reasonable metric.},
	language = {en},
	urldate = {2019-08-15},
	author = {Naulleau, Patrick P. and Goldberg, Kenneth A. and Gullikson, Eric and Mochi, Iacopo and McClinton, Brittany and Rastegar, Abbas},
	editor = {Maurer, Wilhelm and Abboud, Frank E.},
	month = oct,
