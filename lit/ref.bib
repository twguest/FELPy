@incollection{brown_undulators_1992,
	address = {Boston, MA},
	series = {Synchrotron {Radiation} {Research}},
	title = {Undulators and the {Properties} of {Synchrotron} {Radiation}},
	isbn = {978-1-4615-3278-1},
	url = {https://doi.org/10.1007/978-1-4615-3278-1_8},
	abstract = {Relativistic charged-particle beams, when accelerated in a direction transverse to the direction of motion, emit copious quantities of electromagnetic radiation. This radiation is naturally and inevitably emitted when deflection magnets constrain the particles in circular accelerators to their closed orbits, and it is also emitted when charged particles circulate through specific devices known as wigglers and undulators. The radiation is pitched into an angular cone of characteristic angle 1/γ, where γ is the ratio of the kinetic energy of the particle to its rest energyE/mc 2. The spectral properties of the radiation are governed by the nature of the deflection field: for the incoherent sources (bend magnets and wigglers), the radiation is broadband with a characteristic wavelength λ {\textasciitilde} ρ/ γ3, whereρ is the instantaneous radius of curvature; for the periodic sources, the radiation is quasimonochromatic with characteristic wavelength λ{\textasciitilde}λ u /γ2, where λ u is the period of the device. The radiation can be strongly polarized, depending upon the details of the particle acceleration. Finally, the radiation emerges with the pulsed properties of the electron beam, which may involve a time scale of 10 psec or less. For these reasons, synchrotron radiation can be a far more intense source of vacuum ultraviolet, soft X-ray, and hard X-ray radiation than conventional sources, which generally rely upon inner-shell vacancy production or bremsstrahlung by intense electron beams.},
	language = {en},
	urldate = {2019-09-24},
	booktitle = {Synchrotron {Radiation} {Research}: {Advances} in {Surface} and {Interface} {Science}},
