	pages = {60},
	file = {Full Text PDF:/home/twguest/Zotero/storage/A5Z8C4S3/Zdora - 2018 - State of the Art of X-ray Speckle-Based Phase-Cont.pdf:application/pdf;Snapshot:/home/twguest/Zotero/storage/PTQ858BH/60.html:text/html}
