	journal = {Review of Scientific Instruments},
	author = {Fohler, Manuel and Frömmel, Stefanie and Schneider, Michael and Pfau, Bastian and Günther, Christian M. and Hennecke, Martin and Guehrs, Erik and Shemilt, Laura and Mishra, Durgamadhab and Berger, Dirk and Selve, Sören and Mitin, Dmitriy and Albrecht, Manfred and Eisebitt, Stefan},
